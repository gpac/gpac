InitialObjectDescriptor {
  objectDescriptorID 1
  ODProfileLevelIndication 0xFF
  sceneProfileLevelIndication 0xFE
  audioProfileLevelIndication 0xFF
  visualProfileLevelIndication 0xFF
  graphicsProfileLevelIndication 0xFE

  esdescr [
    ES_Descriptor {
      es_id 1
      decConfigDescr DecoderConfigDescriptor {
				streamType 3
				decSpecificInfo BIFSConfig {
				  isCommandStream true
				  pixelMetric false
				  pixelWidth 400
				  pixelHeight 300
				}
      }
    }
  ]
}

Group {
 children [
  DirectionalLight {
   color 1 1 1
  }
  WorldInfo {
   title "LOD test"
   info ["This shows LOD with 2 level of details" "" "GPAC Regression Tests" "$Date: 2007-07-27 09:46:08 $ - $Revision: 1.2 $" "(C) 2002-2004 GPAC Team"]
  }

  Transform {
   children [
    LOD { 
     range [ 4 ]
     center 0 0 0
     level [
      Shape {
       appearance Appearance {
        material Material {
         diffuseColor 1 0 0
        }
       }
       geometry Sphere { radius 1 }
      }
      Shape {
       appearance Appearance {
        material Material {
         diffuseColor 0 1 0
        }
       }
       geometry Box { size 1 1 1 }
      }
     ]
    }
   ]
  }
 ]
}


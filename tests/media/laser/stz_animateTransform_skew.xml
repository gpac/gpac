<?xml version="1.0" encoding="ISO-8859-1"?>
<saf:SAFSession xmlns:saf="urn:mpeg:mpeg4:SAF:2005"  xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:lsr="urn:mpeg:mpeg4:LASeR:2005" xmlns="http://www.w3.org/2000/svg">

<saf:sceneHeader>
   <lsr:LASeRHeader profile="full" resolution="7" timeResolution="1000" coordBits="19"
                    colorComponentBits="8"  useFullRequestHost="true"
                     hasStringIDs="true"/>
</saf:sceneHeader>

<saf:sceneUnit>
   <lsr:NewScene>
      <svg viewBox="0 0 176 208" width="176" height="208">

			    <g id="g1">
							<path id="path" d="M 40,50 C 10 10 140 10 110 50" fill-rule="evenodd" fill="none" stroke="black">
								 <!-- skewY - values -->
							   <animateTransform begin="1" dur="2" attributeName="transform" type="skewY" values="0;45;90;70;50;30;10;0"/>
							</path>
	
				      <rect id="my_rect" x="50" y="30" width="100.5" height="50.3" rx="1.5" ry="2.8">
								 <!-- skewX - from-to -->
				         <animateTransform begin="4" dur="2" attributeName="transform" type="skewX" from="0" to="360"/>
				      </rect>
	
	            <!-- skew pointing to ref ID -->
							<animateTransform xlink:href="#path" begin="7" dur="2" attributeName="transform" type="skewX" values="0;45;90;70;50;30;10;0"/>

			    </g>

      </svg>
   </lsr:NewScene>
</saf:sceneUnit>

<saf:endOfSAFSession/>

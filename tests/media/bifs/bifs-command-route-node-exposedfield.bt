InitialObjectDescriptor {
 objectDescriptorID 1
 audioProfileLevelIndication 255
 visualProfileLevelIndication 254
 sceneProfileLevelIndication 1
 graphicsProfileLevelIndication 1
 ODProfileLevelIndication 1
 esDescr [
  ES_Descriptor {
   ES_ID 1
   decConfigDescr DecoderConfigDescriptor {
    streamType 3
    decSpecificInfo BIFSConfig {
     isCommandStream true
     pixelMetric true
     pixelWidth 300
     pixelHeight 300
    }
   }
  }
 ]
}

DEF OG OrderedGroup {
 children [
  WorldInfo {
   info ["This shows exposed field routing" "GPAC Regression Tests" "$Date: 2007-07-27 09:46:09 $ - $Revision: 1.2 $" "(C) 2002-2004 GPAC Team"]
   title "Field Routing Test"
  }
  DEF BACK Background2D {
   backColor 1 1 1
  }
  Transform2D {
   translation -100 0
   children [
    Shape {
     appearance Appearance {
      material DEF M1 Material2D {
       emissiveColor 1 0 0
       filled TRUE
      }
     }
     geometry Rectangle {
      size 100 50
     }
    }
   ]
  }
  Transform2D {
   translation 100 0
   children [
    Shape {
     appearance Appearance {
      material DEF M2 Material2D {
       emissiveColor 1 1 0
      }
     }
     geometry Circle {
      radius 50
     }
    }
   ]
  }
 ]
}

ROUTE M1.filled TO M2.filled

AT 2000 {
 REPLACE M1.filled BY TRUE
}

AT 4000 {
 REPLACE M1.filled BY FALSE
}

AT 6000 {
 REPLACE M2.filled BY TRUE
}


{
  "name": "gpac-js",
  "version": "12.19.7",
  "license": "LGPL-2.1-or-later",
  "keywords": [
    "media",
    "packaging",
    "video",
    "audio",
    "streaming",
    "gpac",
    "nodejs",
    "bindings"
  ],
  "gypfile": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "index.d.ts",
  "files": [
    "prebuilds/",
    "dist/",
    "binding.gyp",
    "gpac_wrap.cpp",
    "index.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gpac/gpac.git"
  },
  "bugs": {
    "url": "https://github.com/gpac/gpac/issues"
  },
  "homepage": "https://gpac.io",
  "description": "Node.js bindings for GPAC",
  "author": "Team GPAC",
  "scripts": {
    "prepublishOnly": "cp ../../../README.md .",
    "install": "node-gyp-build",
    "prebuild": "prebuildify --strip --napi",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "esbuild --platform=node --format=esm --outfile=dist/index.mjs index.js",
    "build:cjs": "esbuild --platform=node --format=cjs --outfile=dist/index.cjs index.js"
  },
  "dependencies": {
    "node-addon-api": "^8.3.1",
    "node-gyp": "^11.2.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "esbuild": "^0.25.4",
    "prebuildify": "^6.0.1"
  }
}

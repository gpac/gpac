</saf:sceneHeader>

<saf:sceneUnit>
   <lsr:NewScene>
      <svg width="176" height="208" viewBox="0 0 176 208" fill="blue" stroke="red">
      
         <animate xlink:href="#my_text" attributeName="font-weight" values="100;inherit;bold" dur="6"/>

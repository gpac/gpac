<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="mp4client" />
		<Option pch_mode="2" />
		<Option default_target="debug" />
		<Option compiler="gcc" />
		<Build>
			<Target title="release">
				<Option output="..\..\bin\$(TARGET_STR)\MP4Client" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\$(TARGET_STR)\$(PROJECT_NAME)" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-W" />
					<Add option="-fexceptions" />
					<Add directory="..\..\include" />
				</Compiler>
			</Target>
			<Target title="debug">
				<Option output="..\..\bin\$(TARGET_STR)\MP4Client" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\..\bin\$(TARGET_STR)" />
				<Option object_output="obj\$(TARGET_STR)\$(PROJECT_NAME)" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="..\..\modules\demo_is\demo-sensor.bt" />
				<Compiler>
					<Add option="-W" />
					<Add option="-g" />
					<Add option="-fexceptions" />
					<Add directory="..\..\include" />
				</Compiler>
			</Target>
			<Environment>
				<Variable name="TARGET_STR" value="$(TARGET_CC)_$(TARGET_NAME)" />
			</Environment>
		</Build>
		<Linker>
			<Add library="gpac" />
			<Add directory="..\..\bin\$(TARGET_STR)" />
		</Linker>
		<Unit filename="..\..\applications\mp4client\extract.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="..\..\applications\mp4client\main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
